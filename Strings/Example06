/*faça um programa que leia uma string e a imprima de trás para frente. */
#include <stdio.h>
#include <string.h>

int main (void){
    
    char str[50];

    printf("Type your phase: ");
    gets(str);

    for(int i = strlen(str) - 1;i >= 0;--i){
        printf(" %c", str[i]);
    }
    printf("\n");
    return 0;
}
